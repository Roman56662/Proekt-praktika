.header{
  display: flex
  justify-content: space-between
  position: fixed
  width: 100%
  height: 60px
  z-index 20
  background-color: #fff
  .header__logo{
    margin-left 2.2vw
    display: inline-flex
    & .logo__img{
      margin-right 2.2vw
      & img{
        display: block
        height 60px
        width 60px
        border-radius: 100%
        background-color: #FFF45F
      }
    }
    & .logo__title{
      margin: auto auto
    }
  }
  & a{
    font-size 20px
    font-weight bold
    color #000
    }
  & a:hover{
    text-decoration: underline
  }
  .header__nav{
    .navigation{
      display: flex
      height: 100%
      .navigation__list{
        height 100%
        width: 100%
        display: inline-flex
        align-items: center
        & li{
          margin-right 2.2vw
        }

      }
    }
  }
}